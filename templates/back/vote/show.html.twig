{% extends 'back/base_back.html.twig' %}

{% block title %}Vote{% endblock %}

{% block body %}
    <h1>Vote</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vote.id }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ vote.rating }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_vote_index') }}">Retour</a>

    <a href="{{ path('app_back_vote_edit', {'id': vote.id}) }}">Modifier</a>

    {{ include('back/vote/_delete_form.html.twig') }}
{% endblock %}
